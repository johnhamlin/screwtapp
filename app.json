{
  "expo": {
    "name": "screwtapp",
    "slug": "screwtapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          true
        ]
      },
      "bundleIdentifier": "com.johnhamlin.screwtapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "scheme": "myapp",
    "extra": {
      "eas": {
        "projectId": "b22c69ef-c190-4f94-8c2b-bc4c6a9ae4d4"
      }
    },
    "plugins": [
      "expo-router"
    ]
  }
}
