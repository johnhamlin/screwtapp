+
+const moduleName = 'ReduxPersist'
+
+import * as path from 'path'
+
+import pkg from "./package.json"
+
+const banner = `/*!
+  ${moduleName}.js v${pkg.version}
+  ${pkg.homepage}
+  Released under the ${pkg.license} License.
+*/`;
+
+const filePath = 'dist/redux-persist.js'
